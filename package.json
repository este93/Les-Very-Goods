{
  "name": "webpack-yarn",
  "version": "0.0.1",
  "description": "Webpack FrontEnd starter with yarn",
  "main": "index.js",
  "scripts": {
    "dev-local": "webpack-dev-server --hot --progress --colors --config=webpack_config/dev.config.js",
    "dev": "webpack --mode=development --watch --progress --config=webpack_config/dev.config.js",
    "build": "webpack --mode=none --progress --config=webpack_config/build.config.js",
    "prod": "webpack -p --mode=production --progress --config=webpack_config/build.config.js"
  },
  "author": "Bojan Knezevic",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^9.3.1",
    "babel-core": "^6.26.0",
    "babel-loader": "7.1.5",
    "babel-preset-env": "^1.6.1",
    "copy-webpack-plugin": "^4.6.0",
    "css-hot-loader": "^1.3.4",
    "css-loader": "^1.0.1",
    "file-loader": "^2.0.0",
    "imagemin-webpack-plugin": "^2.3.0",
    "jquery": "^3.2.1",
    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "^4.7.2",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "slick-carousel": "^1.8.1",
    "style-loader": "^0.23.1",
    "webpack": "^4.25.1",
    "webpack-build-notifier": "^0.1.21",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.10"
  }
}
